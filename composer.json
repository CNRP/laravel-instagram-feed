{
    "name": "cnrp/laravel-instagram-feed",
    "description": "A custom implementation of Instagram feed for Laravel",
    "type": "library",
    "require": {
        "php": "^8.1",
        "filament/filament": "^3.2",
        "livewire/livewire": "^3.0",
        "spatie/laravel-package-tools": "^1.16.0",
        "illuminate/support": "^10.0",
        "intervention/image": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "CNRP\\InstagramFeed\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "CNRP\\InstagramFeed\\InstagramFeedProvider"
            ]
        }
    }
}